{
    "name": "twitter-art-tags",
    "author": "poohcom1",
    "description": "",
    "version": "2.10.0",
    "private": true,
    "engines": {
        "node": ">=18.20.3",
        "yarn": ">=1.22.22"
    },
    "scripts": {
        "build:prod": "webpack --config webpack.config.js",
        "build:prod:watch": "webpack --config webpack.config.js --watch",
        "build:dev": "webpack --config webpack.dev.js",
        "build:dev:watch": "webpack --config webpack.dev.js --watch",
        "dev": "npm-run-all -p scss:types:watch build:prod:watch",
        "build": "yarn scss:types && yarn build:prod",
        "lint": "eslint",
        "format": "prettier --write src test",
        "test": "jest",
        "scss:types": "typed-scss-modules src",
        "scss:types:watch": "typed-scss-modules src --watch",
        "bump:patch": "yarn version --patch",
        "bump:minor": "yarn version --minor",
        "bump:major": "yarn version --major",
        "postversion": "git push && git push --tags"
    },
    "devDependencies": {
        "@babel/core": "^7.24.7",
        "@eslint/js": "^9.4.0",
        "@types/eslint__js": "^8.42.3",
        "@types/jest": "^29.5.12",
        "@typescript-eslint/eslint-plugin": "^7.12.0",
        "@typescript-eslint/parser": "^7.12.0",
        "babel-loader": "^9.1.3",
        "babel-preset-solid": "^1.8.17",
        "css-loader": "^7.1.2",
        "dotenv-webpack": "^8.1.0",
        "eslint": "^9.4.0",
        "eslint-plugin-only-warn": "^1.1.0",
        "eslint-plugin-unused-imports": "^4.0.0",
        "html-loader": "^5.0.0",
        "jest": "^29.7.0",
        "npm-run-all": "^3.1.1",
        "prettier": "^3.3.2",
        "sass": "^1.77.4",
        "sass-loader": "^14.2.1",
        "style-loader": "^4.0.0",
        "ts-jest": "^29.1.4",
        "ts-loader": "^9.5.1",
        "typed-scss-modules": "^8.0.1",
        "typescript": "^5.4.5",
        "typescript-eslint": "^7.12.0",
        "valibot": "^0.31.0-rc.12",
        "webpack": "^5.91.0",
        "webpack-cli": "^5.0.1",
        "webpack-merge": "^5.10.0",
        "webpack-userscript": "^3.2.2"
    },
    "dependencies": {
        "solid-js": "^1.8.17"
    }
}
